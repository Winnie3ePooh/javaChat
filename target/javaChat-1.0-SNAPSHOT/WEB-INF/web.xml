<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ChatServlet</servlet-name>
        <servlet-class>com.mycompany.javachat.ChatServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.mycompany.javachat.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.mycompany.javachat.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetMess</servlet-name>
        <servlet-class>com.mycompany.javachat.GetMess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TakeMess</servlet-name>
        <servlet-class>com.mycompany.javachat.TakeMess</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChatServlet</servlet-name>
        <url-pattern>/ChatServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetMess</servlet-name>
        <url-pattern>/GetMess/get</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TakeMess</servlet-name>
        <url-pattern>/TakeMess/take</url-pattern>
    </servlet-mapping>
</web-app>
